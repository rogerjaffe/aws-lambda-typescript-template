    "@types/node": "^17.0.21",
    "connect-livereload": "^0.6.1",
    "copyfiles": "^2.4.1",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
