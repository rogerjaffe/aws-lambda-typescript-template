  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -p tsconfig.json",
    "copy": "copyfiles -V -u 1 -e ./src/**.map -e ./src/tests src/*.js src/**/*.js dist/",
    "zip": "cd dist && zip -r ../deploy/function.zip .",
    "upload": "chmod +rx deploy-function.sh && AWS_ENV=PRODUCTION ./deploy-function.sh",
    "set-layer-version": "chmod +rx deploy-layer.sh && ./deploy-layer.sh",
    "deploy": "npm run clean; npm run build; npm run copy; npm run zip; AWS_ENV=PRODUCTION npm run upload",
    "server": "nodemon server.js",
    "test": "jest"
  },
